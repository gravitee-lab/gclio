    "gravitee.io",
    "automation",
    "multi-repo",
    "cicd",
    "development",
    "java",
    "maven",
    "typescript",
    "rxjs"
  ],
  "author": "The Gravitee Devops Team <jean-baptiste.lasselle@graviteesource.com>",
  "license": "AGPL-3.0",
  "bugs": {
    "url": ""
  },
  "homepage": "https://github.com/gravitee-lab/gclio#README.md",
