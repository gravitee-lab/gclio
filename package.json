    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "typescript-collections": "^1.3.3",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0",
    "yargs": "^16.0.3"
