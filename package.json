{
  "name": "gclio",
  "version": "0.0.1",
  "description": "The https://gravitee.io Dev Team CLI",
  "main": "index.js",
  "bin": {
    "gclio": "./bin/index.js"
  },
  "scripts": {
    "test": "echo 'no tests yet'",
    "dev": "npm run build && ./node_modules/ts-node/bin/bin.js ./src/index.ts",
    "start": "node ./bin/index.js",
    "doc": "./node_modules/typedoc/bin/typedoc --out docs/ ./src --theme node_modules/eledoc/bin/default/",
    "compile": "tsc ",
    "build": "npm run compile && npm run test && npm run doc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gravitee-lab/gclio.git"
  },
  "keywords": [
